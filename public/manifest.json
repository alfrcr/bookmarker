{
	"manifest_version": 2,
	"name": "Prakerja Gratis Bookmarker",
	"description": "Bookmarker tools to seed prakerjagratis.com",
	"version": "1",
	"author": "BukanStafsus",
	"browser_action": {
		"default_title": "Prakerja Gratis",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"tabs"
	],
	"icons": {
		"16": "logo192.png",
		"48": "logo192.png",
		"128": "logo512.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-M/5ldsBDkbFDZ++a048KPcfiM23RMqNfThQV1BRAZNk='; object-src 'self'"
}